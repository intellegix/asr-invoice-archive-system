# ASR Records Legacy Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_VERSION=v1

# Application Configuration
VITE_APP_TITLE=ASR Records Legacy
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Simplified Document Management System

# Multi-tenant Configuration
VITE_TENANT_ID=demo-tenant
VITE_TENANT_NAME=Demo Company

# Feature Flags
VITE_ENABLE_UPLOAD=true
VITE_ENABLE_CLASSIFICATION=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_ANALYTICS=true

# Claude AI Configuration
VITE_CLAUDE_MODEL=claude-3-sonnet-20240229
VITE_CLAUDE_MAX_TOKENS=4000

# Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_MAX_FILES=10
VITE_ACCEPTED_TYPES=.pdf,.png,.jpg,.jpeg

# Development
VITE_DEBUG=false
VITE_LOG_LEVEL=info

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_MIXPANEL_TOKEN=

# Sentry (Optional)
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development